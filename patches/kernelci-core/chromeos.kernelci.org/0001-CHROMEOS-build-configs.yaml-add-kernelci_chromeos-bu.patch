From 59c6c6d2632d6563ddcd0450e39e46a48d8cd963 Mon Sep 17 00:00:00 2001
From: Guillaume Tucker <guillaume.tucker@collabora.com>
Date: Thu, 14 Feb 2019 15:16:26 +0000
Subject: [PATCH 1/4] CHROMEOS build-configs.yaml: add kernelci_chromeos-*
 build configs

---
 config/core/build-configs.yaml | 45 ++++++++++++++++++++++++++++++++++
 1 file changed, 45 insertions(+)

diff --git a/config/core/build-configs.yaml b/config/core/build-configs.yaml
index c87be20c030c..84216022cf71 100644
--- a/config/core/build-configs.yaml
+++ b/config/core/build-configs.yaml
@@ -774,6 +774,51 @@ build_configs:
     branch: 'kernelci.org'
     variants: *stable_variants
 
+  kernelci_chromeos-mainline:
+    tree: kernelci
+    branch: 'chromeos-mainline'
+    variants:
+      gcc-8: &chromeos_gcc
+        build_environment: gcc-8
+        fragments: [kselftest]
+        architectures:
+          arc: *arc_defconfig
+          arm: *arm_defconfig
+          arm64: *arm64_defconfig
+          i386: *i386_defconfig
+          mips: *mips_defconfig
+          riscv: *riscv_defconfig
+          x86_64:
+            <<: *x86_64_defconfig
+            fragments: [x86-chromebook]
+            extra_configs: ['x86_64_defconfig+x86-chromebook+kselftest']
+      clang-10: &chromeos_clang
+        build_environment: clang-10
+        fragments: [kselftest]
+        architectures:
+          arm: *arm_defconfig
+          arm64: *arm64_defconfig
+          x86_64:
+            <<: *x86_64_defconfig
+            fragments: [x86-chromebook]
+            extra_configs: ['x86_64_defconfig+x86-chromebook+kselftest']
+
+  kernelci_chromeos-next:
+    tree: kernelci
+    branch: 'chromeos-next'
+    variants:
+      gcc-8: *chromeos_gcc
+      clang-13:
+        <<: *chromeos_clang
+        build_environment: clang-13
+
+  kernelci_chromeos-stable:
+    tree: kernelci
+    branch: 'chromeos-stable'
+    variants:
+      gcc-8: *chromeos_gcc
+      clang-10: *chromeos_clang
+
   khilman:
     tree: khilman
     branch: 'to-build'
-- 
2.20.1

