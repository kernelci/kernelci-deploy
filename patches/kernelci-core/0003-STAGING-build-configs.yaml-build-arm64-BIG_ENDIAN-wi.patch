From b63617a948a60355fbd5586824b2546c8f28642d Mon Sep 17 00:00:00 2001
From: "kernelci.org bot" <bot@kernelci.org>
Date: Thu, 20 Feb 2020 19:47:01 +0000
Subject: [PATCH] STAGING build-configs.yaml: build arm64 BIG_ENDIAN with gcc
 and clang

---
 build-configs.yaml | 8 ++++++--
 1 file changed, 6 insertions(+), 2 deletions(-)

diff --git a/build-configs.yaml b/build-configs.yaml
index f18e55052fdb..b0652b4f9d64 100644
--- a/build-configs.yaml
+++ b/build-configs.yaml
@@ -538,10 +538,12 @@ build_configs:
             base_defconfig: 'multi_v7_defconfig'
             filters:
               - whitelist: {defconfig: ['multi_v7_defconfig']}
-          arm64: &staging_arm64
+          arm64:
             filters:
               - whitelist: {defconfig: ['defconfig']}
             fragments: [kselftest]
+            extra_configs:
+              - 'defconfig+CONFIG_CPU_BIG_ENDIAN=y'
           i386:
             base_defconfig: 'i386_defconfig'
             filters:
@@ -561,7 +563,9 @@ build_configs:
         build_environment: clang-9
         architectures:
           arm: *staging_arm
-          arm64: *staging_arm64
+          arm64:
+            extra_configs:
+              - 'defconfig+CONFIG_CPU_BIG_ENDIAN=y'
           x86_64: *staging_x86_64
 
